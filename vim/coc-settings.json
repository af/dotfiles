{
  "coc.source.emoji.enable": true,
  "coc.source.emoji.filetypes": ["markdown", "css", "javascript", "typescript"],
  "coc.preferences.extensionUpdateCheck": "never",
  "suggest.noselect": false,
  "diagnostic.displayByAle": true,
  "snippets.loadFromExtensions": false,
  "snippets.userSnippetsDirectory": "~/.vim/personal_snippets",
  "snippets.extends": {
    "javascriptreact": [
      "javascript"
    ],
    "typescript": [
      "javascript"
    ],
    "typescriptreact": [
      "javascript"
    ]
  },
  "tsserver.enableJavascript": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "typescript.suggest.completeFunctionCalls": false,
  "languageserver": {
    "reason": {
      "command": "~/code/reason/reason-language-server.exe",
      "filetypes": [
        "reason"
      ]
    }
  },

  "list.nextKeymap": "<C-n>",
  "list.previousKeymap": "<C-p>",
  "list.maxHeight": 15,
  "list.insertMappings": {
    "<C-c>": "do:exit"
  }
}
